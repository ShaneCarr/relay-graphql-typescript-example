{
  "name": "relay-graphql-typescript-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel start:server start:client",
    "start:server": "nodemon --exec ts-node src/server.ts",
    "start:client": "cd client && npm start",
    "build": "npm run relay && npm-run-all build:client",
    "build:client": "webpack --config webpack.prod.config.js",
    "build:server": "tsc -p tsconfig.server.json",
    "start:prod": "npm-run-all build:server build:client start:server:prod",
    "start:server:prod": "node dist/server.js",
    "relay": "npx relay-compiler"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mui/icons-material": "^6.1.0",
    "apollo-server-express": "^3.13.0",
    "express": "^4.21.0",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/express": "4.17.14",
    "@types/node": "^22.4.1",
    "babel-loader": "^9.1.3",
    "eslint": "^9.9.0",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "relay-compiler": "^17.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
